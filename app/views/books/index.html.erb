<h1>Books</h1>

  <table class='table'>
    <thead>
      <tr>
        <th>Title</th>

        <th>Option</th>
      </tr>
    </thead>

    <tbody>
     <% @books.each do |book| %>
     
      <tr>
        <td><%= book.title %></td>

        <td><%= book.body %></td>
      </tr>
     <% end %>
    </tbody>
  </table>

